<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>ControllerServlet</servlet-name>
        <servlet-class>ru.adduxa.inetApps.Lab7.ControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerServlet</servlet-name>
        <url-pattern>/ControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>WebFormServlet</servlet-name>
        <servlet-class>ru.adduxa.inetApps.Lab7.WebFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebFormServlet</servlet-name>
        <url-pattern>/WebFormServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AreaCheckServlet</servlet-name>
        <servlet-class>ru.adduxa.inetApps.Lab7.AreaCheckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AreaCheckServlet</servlet-name>
        <url-pattern>/AreaCheckServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>ControllerServlet</welcome-file>
    </welcome-file-list>
</web-app>